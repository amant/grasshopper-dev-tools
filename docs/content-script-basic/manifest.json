{
  "name": "Content Script Basic",
  "version": "0.1.0",
  "description": "A basic content script example extension",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_idle"
    }
  ]
}
